sno=int(input("Enter sno"))
sname=input("Enter student name")
group=input("Enter group")
s1=int(input("Enter java marks"))
s2=int(input("Enter os marks"))
s3=int(input("Enter accounts marks"))
total=s1+s2+s3
print(total)
avg=total/3
print(avg)
if avg>=90:
    print("O-Grade")
elif avg>=80:
    print("A-Grade")
elif avg>=70:
    print("B-Grade")
elif avg>=60:
    print("C-Grade")
elif avg>=50:
    print("D-Grade")
else:
    print("Pass")
